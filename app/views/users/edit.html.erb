<h2>Edit Your Profile</h2>

<form action="/users/<%= current_user.id%>" method="post">
<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
<input type="hidden" name="_method" value="patch">
First Name:<input type="text" name="user[first_name]" value="<%= current_user.first_name %>"><br><br>
Last Name:<input type="text" name="user[last_name]" value="<%= current_user.last_name %>"><br><br>
Email:<input type="text" name="user[email]" value="<%= current_user.email %>"><br><br>
City:<input type="text" name="user[location]" value="<%= current_user.location %>">
State:<select class="" name="user[state]">
  <option value="<%= current_user.state %>"><%= current_user.state %></option>
  <option value="IL">IL</option>
  <option value="NY">NY</option>
  <option value="CA">CA</option>
  <option value="WY">WY</option>
</select><br><br>
<input type="submit" name="" value="Update Records"><br><br>
</form>
